    <tr>
      <th>Weather Code</th>
      <th>Temp</th>
      <th>Wind Speed</th>
      <th>Wind Direction</th>
      <th>Pressure</th>
    </tr>
  </thead>
  <tbody>
    {{#each station.readings}}
    <tr>
      <td>{{code}}</td>
      <td>{{temp}}</td>
      <td>{{windSpeed}}</td>
      <td>{{windDirection}}</td>
      <td>{{pressure}}</td>
    </tr>
    <tr>
      <td>{{code}}</td>
